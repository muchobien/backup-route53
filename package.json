{
  "name": "@muchobien/backup-route53",
  "version": "0.0.1",
  "private": true,
  "description": "A Github action to backup Route53 records",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muchobien/backup-route53.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Rodrigo Sanabria <rodrigo@sanabria.dev>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@aws-sdk/client-route-53": "3.245.0"
  },
  "devDependencies": {
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@typescript-eslint/parser": "5.48.1",
    "@vercel/ncc": "0.36.0",
    "aws-sdk-client-mock": "2.0.1",
    "eslint": "8.32.0",
    "eslint-plugin-github": "4.6.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.3.1",
    "js-yaml": "4.1.0",
    "prettier": "2.8.2",
    "ts-jest": "29.0.4",
    "typescript": "4.9.4"
  }
}
